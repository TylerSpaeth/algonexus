<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane fx:id="borderPane" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.tylerspaeth.ui.controllers.MainController">
   <bottom>
   </bottom>
   <center>
       <fx:include source="Home.fxml" />
   </center>
   <top>
      <ButtonBar prefHeight="30.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
            <Button fx:id="homeButton" mnemonicParsing="false" onAction="#homeButtonClicked" text="Home" />
          <Button fx:id="accountButton" mnemonicParsing="false" onAction="#accountButtonClicked" text="Account" />
            <Button fx:id="strategiesButton" mnemonicParsing="false" onAction="#strategiesButtonClicked" text="Strategies" />
        </buttons>
      </ButtonBar>
   </top>

      </BorderPane>
